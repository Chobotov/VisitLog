<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Alert" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/textColor</item>
    </style>

    <style name="Tab">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#6A6868</item>
    </style>

    <style name="Floating" parent="AppTheme">
        <item name="colorAccent">@color/floatingButtonBG</item>
        <item name="colorPrimary">@color/floatingButtonPlus</item>
    </style>

   >


</resources>
